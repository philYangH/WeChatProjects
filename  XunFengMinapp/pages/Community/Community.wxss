page{
    background-color: #F5F5F5;
}
.wrapper image{
    width: 100%;
}
.store{
    text-align: center;
}
#storeSwiper{
    margin: auto;
    margin-top: 20rpx;
    background-color: white;
    border-radius: 10rpx;
    width: 95%;
    height: 341prx;
}
.swiper-image image{
    border-radius: 30rpx;
    width: 95%;
    height: 341prx;
}
#searchInput{
    position: relative;
    width: 95%;
    color: gray;
    margin: auto;
    background-color: white;
    margin-top: 20rpx;
    border-radius: 20rpx;
    height: 45rpx;
}
#searchInput image{
    width: 30rpx;
}
#catagory{
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 90%;
    color: gray;
    margin: auto;
    padding: 15rpx;
    height: 152rpx;
    background-color: white;
    margin-top: 20rpx;
    border-radius: 20rpx;
}
#catagory image{
    width: 67rpx
}
#goods{
    width: 99%;
    margin: auto;
    background-color: white;
    margin-top: 20rpx;
    border-radius: 20rpx;
}
#goodsTitle image{
    padding-top: 35.5rpx;
    width: 208rpx;
}
#goodsDisplay{
    display: inline;
}
#goodsDisplay image{
    width: 335rpx;
}
